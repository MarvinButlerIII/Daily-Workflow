// Search inside the variable.

var letters = 'abc';
letters.match(/a/);

// another example where null appears to indicate an object
var letters = 'abc';
letters.match(/d/); // null appears instead to indicate the absence of an object

// Undefined data type

console.log('Jane Smith'); // get undefined because console.log is a function and you are not returning a value.

// Another example where a undifed data type appears

var noise; // automatically assigned as undefined
console.log(noise);
noise = 'thunder';
console.log(noise); // the assinged value returns thunder

// Cant console.log a variable it'll throw a 'RefferenceError'

//console.log(noise2);

// Undefined when you try to access an object property that doesn't exist

var game = {
    score: 1000
}
game.Score; // spelling error of properties spelling score with capital S gives the value undefined instead.

// Empty String example

var name1 = '';
var name2 = ""; // can be built with single quotes '' or double quotes "".